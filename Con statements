public class Main{
    public static void main(String[] args){
        int age = 15;
        if(age>=18)
        {
            System.out.println("Eligible for voting");
        }
        else if(age==18)
        {
            System.out.println("Eligible for voting");
        }
        else
        {
            System.out.println("Not eligible for voting");
        }
    }
}
