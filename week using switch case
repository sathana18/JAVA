public class main{
public static void main(String[] args){
        int days = 4;
        switch(days)
        {
            case 1:
                System.out.println("Sunday");
                break;
            case 4:
                System.out.println("Wednesday");
                break;
            case 7:
                System.out.println("Saturday");
                break;
            case 9:
                System.out.println("Invalid input.Please enter a value between 1 to 7.");
                break;
            default:
                System.out.println("invalid");
        }
    }
}
